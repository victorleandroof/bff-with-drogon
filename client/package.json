{
  "name": "bff-login",
  "version": "1.0.38",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --dest ../bff/assets/login",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint '{src,spec}/**/*.{ts,vue}' ",
    "sass-lint": "node_modules/.bin/sass-lint-vue src",
    "test": "./node_modules/.bin/jest --detectOpenHandles",
    "test:ci": "npm run test -- --ci",
    "test:watch": "npm run test -- --watch",
    "test:cov": "npm run test -- --coverage",
    "test:debug": "node --inspect-brk node_modules/.bin/vue-cli-service test:unit --no-cache --watch --runInBand"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bootstrap": "^4.6.0",
    "bootstrap-vue": "^2.21.2",
    "core-js": "^3.6.5",
    "vue": "^2.6.14",
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.2.0",
    "vue-toastification": "^1.7.11"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/jest": "^26.0.15",
    "@types/jsdom": "^16.2.13",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "^3.1.1",
    "@vue/cli-plugin-router": "~4.4.0",
    "@vue/cli-plugin-typescript": "~4.4.0",
    "@vue/cli-plugin-unit-jest": "^4.5.0",
    "@vue/cli-service": "^3.12.1",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^1.0.3",
    "assets-webpack-plugin": "^7.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^6.2.2",
    "jest": "^26.6.3",
    "jest-fetch-mock": "^3.0.3",
    "jest-junit": "^12.2.0",
    "jsdom": "^16.6.0",
    "node-sass": "^6.0.1",
    "prettier": "^2.2.1",
    "sass": "^1.35.2",
    "sass-lint-vue": "^0.4.0",
    "sass-loader": "10.1.1",
    "style-resources-loader": "^1.4.1",
    "ts-jest": "^26.4.3",
    "ts-node": "^10.1.0",
    "typescript": "~4.1.5",
    "vue-cli-plugin-style-resources-loader": "^0.1.3",
    "vue-jest": "^3.0.6",
    "vue-template-compiler": "^2.6.11"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "vue"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest",
      ".*\\.(vue)$": "vue-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "reporters": [
      "default",
      "jest-junit"
    ],
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testURL": "http://localhost/",
    "collectCoverage": true,
    "coverageDirectory": "../.ci/reports/client",
    "coverageReporters": [
      "text",
      "clover",
      "lcov",
      "json-summary"
    ],
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.ts",
      "<rootDir>/src/**/*.vue",
      "!<rootDir>/src/**/*.d.ts",
      "!<rootDir>/src/router/index.ts",
      "!<rootDir>/src/main.ts",
      "!<rootDir>/src/stylesheet/theme.ts"
    ]
  }
}
